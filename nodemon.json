{
  "verbose": true,
  "ignore": [],
  "watch": [
    ".env",
    ".eslintrc",
    "config-overrides.js",
    "next.config.js",
    "nodemon.json",
    "package.json",
    "public/*",
    "src/*",
    "node_modules/*"
